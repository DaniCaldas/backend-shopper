generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Costumer {
  costumer_code String     @id
  measures      Measures[]
}

model Measures {
  measure_uuid          String   @id
  measure_datetime      DateTime
  measure_type          String
  has_confirmed         Boolean
  image_url             String
  costumer_id           String
  costumer              Costumer @relation(fields: [costumer_id], references: [costumer_code])
}
